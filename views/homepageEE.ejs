<link rel="stylesheet" href="/stylesheets/homepageEE.css" type="text/css" />
<link rel="stylesheet" href="/stylesheets/alunos.css" type="text/css" />



<head>
    <meta charset="utf-8">
    <title>CaderNet </title>
    <link rel="stylesheet" href="http://fonts.googleapis.com/css?family=Open+Sans:400,300,700|Raleway:300,400,500,700">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css">
    <link rel="stylesheet" href="css/style.css">


</head>

<body>

    <div class="headerBar">
        <div class="headerLeft">
            <p></p>
        </div>
        <div>
            <form action="/logout" method="post">
                <button class="btnlog sairHome" type="submit">
                    <p class="textSair ">Sair</p>
                </button>
            </form>
        </div>
    </div>


    <div class="wrapper-grey padded">
        <div class="container">
            <h2 class="text-center">Os Meus Educandos</h2>

            <% alunos.forEach(function (aluno) { %>
                <div class="col-xs-12 col-sm-4">
                    <div class="card"
                        style="background: linear-gradient(rgba(49, 84, 238, 0.356), rgba(166, 179, 238, 0.356))">
                        <div class="btnscontainer">
                            <button type="button" class="btnNewR"
                                onclick="window.location='/homepage/EE/<%= aluno.id %>/relatorios'">Relat√≥rios</button>
                            <button type="button" class="btnNewR"
                                onclick="window.location='/homepage/EE/<%= aluno.id %>/mensagens'">Mensagens</button>
                        </div>
                        <div class="card-description">
                            <h2>
                                <%= aluno.nome_aluno %>
                            </h2>
                        </div>

                    </div>
                </div>
                <% }) %>

        </div>
    </div>


    <!-- Including Bootstrap JS (with its jQuery dependency) so that dynamic components work -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.2.0/js/bootstrap.min.js"></script>
</body>